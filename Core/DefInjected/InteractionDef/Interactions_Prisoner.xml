  <!-- EN: recruit attempt -->
  <RecruitAttempt.label>încercare de recrutare</RecruitAttempt.label>
  <!-- EN:
    <li>r_logentry->[INITIATOR_nameDef] asked [RECIPIENT_nameDef] to join.</li>
    <li>r_logentry->[INITIATOR_nameDef] promised [goodthing] to [RECIPIENT_nameDef], and asked [RECIPIENT_objective] to join.</li>
    <li>goodthing(p=15)->[ConceptPositive]</li>
    <li>goodthing->free, fresh [Vegetable]s</li>
  -->
  <RecruitAttempt.logRulesInitiator.rulesStrings>
    <li>r_logentry->[INITIATOR_nameDef] i-a propus lui [RECIPIENT_nameDef] să ni se alăture.</li>
    <li>r_logentry->[INITIATOR_nameDef] i-a promis [goodthing] către [RECIPIENT_nameDef], și i-a propus [RECIPIENT_objective] să ni se alăture.</li>
    <li>goodthing(p=15)->[ConceptPositive]</li>
    <li>goodthing->[Vegetable] gratis și proaspete</li>
  </RecruitAttempt.logRulesInitiator.rulesStrings>
  
  <!-- EN: spark jailbreak -->
  <SparkJailbreak.label>pornește evadarea din închisoare</SparkJailbreak.label>
  <!-- EN:
    <li>r_logentry->[INITIATOR_nameDef] spoke about the joy of [freedom], and offered a means of escape.</li>
    <li>r_logentry->[INITIATOR_nameDef] talked about how beautiful the [TerrainFeature] is, and offered freedom.</li>
    <li>r_logentry->[INITIATOR_nameDef] demonstrated a means of escape to [RECIPIENT_nameDef], and promised [freedom].</li>
    <li>r_logentry->[INITIATOR_nameDef] told [RECIPIENT_nameDef] that security is lax, and encouraged escape.</li>
    <li>freedom->freedom</li>
    <li>freedom->opportunity</li>
    <li>freedom->home</li>
    <li>freedom->self-determination</li>
  -->
  <SparkJailbreak.logRulesInitiator.rulesStrings>
    <li>r_logentry->[INITIATOR_nameDef] a vorbit despre bucuria [freedom] și a oferit o cale de scăpare.</li>
    <li>r_logentry->[INITIATOR_nameDef] a vorbit despre cât de frumos este [TerrainFeature] și a oferit libertate.</li>
    <li>r_logentry->[INITIATOR_nameDef] a demonstrat un mijloc de evadare lui [RECIPIENT_nameDef] și i-a promis șansa [freedom].</li>
    <li>r_logentry->[INITIATOR_nameDef] i-a spus lui [RECIPIENT_nameDef] că securitatea este laxă și a încurajat evadarea.</li>
    <li>freedom->de libertate</li>
    <li>freedom->de oportunitate</li>
    <li>freedom->de a fi acasă</li>
    <li>freedom->de autodeterminare</li>
  </SparkJailbreak.logRulesInitiator.rulesStrings>
  
